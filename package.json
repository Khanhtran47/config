{
  "$schema": "https://json.schemastore.org/package",
  "name": "@khanhtran47/config",
  "version": "1.0.9",
  "description": "My base ESLint, prettier config for JS/TS projects",
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config",
    "typescript",
    "typescript-config"
  ],
  "author": {
    "name": "Duc Khanh TRAN",
    "email": "tranduckhanh23@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "imports": {
    "#/*": "./*"
  },
  "exports": {
    ".": "./index.js",
    "./prettier": "./prettier.js",
    "./typescript": "./typescript.json",
    "./reset.d.ts": "./reset.d.ts",
    "./eslint": "./eslint.js"
  },
  "homepage": "https://github.com/Khanhtran47/config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Khanhtran47/config.git"
  },
  "bugs": {
    "url": "https://github.com/Khanhtran47/config/issues"
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "eslint .",
    "typecheck": "tsc",
    "validate": "run-p -l format:check lint typecheck"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@eslint/js": "9.35.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@vitest/eslint-plugin": "1.3.9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.2",
    "eslint-plugin-storybook": "9.1.5",
    "eslint-plugin-testing-library": "7.7.0",
    "globals": "16.4.0",
    "prettier-plugin-sort-json": "4.1.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tslib": "2.8.1",
    "typescript-eslint": "8.43.0"
  },
  "devDependencies": {
    "@playwright/test": "1.55.0",
    "@types/node": "22.15.17",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "eslint": "9.35.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}
