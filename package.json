{
  "name": "@khanhtran47/config",
  "version": "1.0.0",
  "description": "My base ESLint, prettier config for JS/TS projects",
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config",
    "typescript",
    "typescript-config"
  ],
  "author": {
    "name": "Duc Khanh TRAN",
    "email": "tranduckhanh23@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "imports": {
    "#/*": "./*"
  },
  "exports": {
    ".": "./index.js",
    "./prettier": "./prettier.js",
    "./typescript": "./typescript.json",
    "./reset.d.ts": "./reset.d.ts",
    "./eslint": "./eslint.js"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/Khanhtran47/config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Khanhtran47/config.git"
  },
  "bugs": {
    "url": "https://github.com/Khanhtran47/config/issues"
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "eslint .",
    "typecheck": "tsc",
    "validate": "run-p -l format:check lint typecheck"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engine": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@eslint/js": "9.26.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@vitest/eslint-plugin": "1.1.44",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-import-x": "4.11.1",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-playwright": "2.2.0",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "7.1.1",
    "globals": "16.1.0",
    "prettier-plugin-sort-json": "4.1.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tslib": "^2.8.1",
    "typescript-eslint": "8.32.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@playwright/test": "^1.52.0",
    "@types/node": "22.15.17",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "eslint": "9.26.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vitest": "^3.1.3"
  }
}
